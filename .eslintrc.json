{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["next/core-web-vitals", "airbnb-base"],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint",
        "react"
    ],
    "rules": {
        "no-unused-vars": "warn"
    }
}
